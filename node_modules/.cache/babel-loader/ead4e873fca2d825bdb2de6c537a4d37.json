dex) in ArrayNew\"\n                :key=\"(Product, index)\"\n                class=\"woocommerce-cart-form__cart-item cart_item\"\n              >\n                <div class=\"product-remove\">\n                  <a\n                    href=\"\"\n                    class=\"remove\"\n                    aria-label=\"Xóa sản phẩm này\"\n                    data-product_id=\"900\"\n                    data-product_sku=\"121186-1\"\n                    >×</a\n                  >\n                </div>\n\n                <div class=\"product-thumbnail\">\n                  <a href=\"\"\n                    ><img\n                      width=\"300\"\n                      height=\"225\"\n                      :src=\"Product.img\"\n                      class=\"attachment-woocommerce_thumbnail size-woocommerce_thumbnail\"\n                      alt=\"\"\n                  /></a>\n                </div>\n\n                <div class=\"product-name\" data-title=\"Sản phẩm\">\n                  <a href=\"\" class=\"\">{{ Product.title }}</a>\n                </div>\n\n                <div class=\"product-price\" data-title=\"Giá\">\n                  <span class=\"woocommerce-Price-amount amount font-medium\">{{\n                    Product.TextCost\n                  }}</span>\n                </div>\n\n                <div class=\"product-quantity\" data-title=\"Số lượng\">\n                  <div class=\"quantity buttons_added\">\n                    <span\n                      @click=\"countProduct--\"\n                      class=\"button-min bg-[rgb(249,249,249)] w-[20px] text-center leading-[35px] h-[35px] border-[#ccc]\"\n                      >-</span\n                    >\n                    <span\n                      class=\"button-number leading-[33px] text-center w-[35px] bg-[#FFFFFF]\"\n                      >{{ countProduct }}</span\n                    >\n                    <span\n                      @click=\"countProduct++\"\n                      class=\"button-max bg-[#F9F9F9] w-[20px] text-center h-[35px] leading-[35px]\"\n                      >+</span\n                    >\n                  </div>\n                </div>\n\n                <div class=\"product-subtotal\" data-title=\"Tổng\">\n                  <span class=\"woocommerce-Price-amount amount font-medium\">{{\n                    Product.TextCost\n                  }}</span>\n                </div>\n              </div>\n            </div>\n            <div\n              class=\"around-button-update mt-[30px] text-[#C30005] mb-[50px] flex-row flex justify-start\"\n            >\n              <span\n                class=\"btn-continue text-[14px] font-medium mr-[20px] px-[10px] py-[8px]\"\n                >TIẾP TỤC XEM SẢN PHẨM</span\n              >\n              <span\n                class=\"btn-update text-[14px] font-medium px-[10px] py-[8px]\"\n                >CẬP NHẬT GIỎ HÀNG</span\n              >\n            </div>\n          </div>\n          <div\n            class=\"around-payment-sum xs:w-full lg:w-[40%] xs:pr-[20px] pl-[20px]\"\n          >\n            <h1 class=\"sum-number text-[16px] text-left font-medium\">\n              TỔNG SỐ LƯỢNG\n            </h1>\n            <div\n              class=\"around-sum-phu flex mt-[10px] text-[14px] justify-between pb-[10px]\"\n            >\n              <span>Tổng phụ</span>\n              <span class=\"font-medium\">{{ FormatSum }}</span>\n            </div>\n            <div\n              class=\"around-ship flex mt-[10px] text-[14px] pb-[10px] justify-between items-center text-[#6B6866]\"\n            >\n              <span>Giao hàng</span>\n              <ul>\n                <li>Giao hàng miễn phí</li>\n                <li>Ước tính cho Việt Nam</li>\n                <li>Đổi địa chỉ</li>\n              </ul>\n            </div>\n            <div\n              class=\"around-sum-center flex justify-between mt-[10px] text-[14px]\"\n            >\n              <span>Tổng</span>\n              <span class=\"font-medium\">{{ FormatSum }}</span>\n            </div>\n            <button\n              class=\"mt-[20px] text-[16px] font-medium text-white bg-[#D26E4B] w-full h-[40px]\"\n            >\n              TIẾN HÀNH THANH TOÁN\n            </button>\n            <div\n              class=\"around-card-discount mt-[20px] flex-row justify-start flex items-center pb-[10px]\"\n            >\n              <i class=\"fa-brands fa-discourse mr-[10px] text-[18px]\"></i>\n              <span class=\"font-medium\">Phiếu ưu đãi</span>\n            </div>\n            <input\n              class=\"input-discount w-full h-[40px] mt-[10px] pl-[15px] outline-none\"\n              placeholder=\"Mã ưu đãi\"\n              type=\"text\"\n            />\n            <button\n              class=\"btn-discount w-full mt-[20px] h-[40px] bg-[#F9F9F9] font-normal text-[#796866]\"\n            >\n              Áp dụng\n            </button>\n          </div>\n        </div>\n      </div>\n      <Footer />\n      <Loader class=\"load\" />\n    </div>\n  </div>\n</template>\n\n<script>\nimport Loader from \"../components/Loader.vue\";\nimport Header from \"../components/Header.vue\";\nimport Footer from \"../components/Footer.vue\";\nimport { mapState } from \"vuex\";\nexport default {\n  computed: {\n    ...mapState([\"ArrayNew\", \"FormatSum\"]),\n  },\n  data() {\n    return {\n      countProduct: 1,\n    };\n  },\n  components: {\n    Header,\n    Footer,\n    Loader,\n  },\n  mounted() {\n    var number = 1;\n    this.countProduct = number;\n    var min = document.querySelector(\".button-min\");\n    var plus = document.querySelector(\".button-max\");\n    var input_number = document.querySelector(\".button-number\");\n    // min.addEventListener('click',function(){\n    // number--;\n    // });\n    // plus.addEventListener('click', function(){\n    //  number++;\n    // })\n    var load = document.querySelector(\".load\");\n\n    setTimeout(function () {\n      load.style.display = \"none\";\n    }, 1000);\n  },\n};\n</script>\n\n<style scoped>\n.list-payment {\n  display: grid;\n  grid-template-columns: 2fr 1fr 1fr 1fr;\n}\ntr {\n  display: table-row;\n  vertical-align: inherit;\n  border-color: inherit;\n}\ntbody {\n  display: table-row-group;\n  vertical-align: middle;\n  border-color: inherit;\n}\n.product-thumbnail {\n  min-width: 60px;\n  max-width: 90px;\n  width: 90px;\n}\n.remove {\n  display: block;\n  width: 24px;\n  height: 24px;\n  font-size: 15px !important;\n  line-height: 19px !important;\n  border-radius: 100%;\n  color: #ccc;\n  font-weight: bold;\n  text-align: center;\n  border: 2px solid currentColor;\n}\n.shop_table .cart_item td {\n  padding-top: 15px;\n  padding-bottom: 15px;\n}\ntd {\n  padding: 0.5em;\n  text-align: left;\n  border-bottom: 1px solid #ececec;\n  line-height: 1.3;\n  font-size: 0.9em;\n}\ntd.product-name {\n  word-break: break-word;\n  text-overflow: ellipsis;\n}\n\n.text-center .quantity,\n.quantity {\n  opacity: 1;\n  display: inline-block;\n  display: -ms-inline-flexbox;\n  display: inline-flex;\n  margin-right: 1em;\n  white-space: nowrap;\n  vertical-align: top;\n}\n.screen-reader-text {\n  clip: rect(1px, 1px, 1px, 1px);\n  position: absolute !important;\n  height: 1px;\n  width: 1px;\n  overflow: hidden;\n}\n\n.quantity .minus,\n.quantity .plus {\n  padding-left: 0.5em;\n  padding-right: 0.5em;\n}\n\n.button-min {\n  border: 1px solid #ccc;\n  box-shadow: 0 0 0.2px 0.3px #ccc;\n}\n.button-number {\n  border-top: 1px solid #ccc;\n  border-bottom: 1px solid #ccc;\n  box-shadow: 1px 0 1px 0 #ccc;\n}\n.button-max {\n  border: 1px solid #ccc;\n  box-shadow: 0 0 0.2px 0.3px #ccc;\n}\n.btn-continue {\n  border: 3px solid #c30005;\n}\n.btn-update {\n  border: 3px solid #c30005;\n}\n.list-product-title {\n  border-bottom: 3px solid #cccc;\n}\n.around-payment-left {\n  border-right: 1px solid #ccc;\n}\n.sum-number {\n  padding-bottom: 10px;\n  border-bottom: 2px solid